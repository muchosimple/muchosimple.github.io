<div class="page">
  {{> organisms-header }}

  {{# has-top-ad }}
    <!-- <hr class="no-space"> -->
  {{/ has-top-ad }}

  <div class="main cf" role="main" aria-live="polite">
    <div class="lc lc-inner--l cf page--top">
      <div class="shift-left">
        <div class="shift-left-inner">
          <div class="carousel--tabbed js-carousel--tabbed">
            {{# home-carousel }}
              <div class="c-item c-tabbed c-item-{{ item-num }}">
                {{> molecules-image-with-social }}
                <a href="{{ url }}" class="c-tab">
                  <h2 class="c-title">{{{ tab-title }}}</h2>
                </a> <!-- /.c-tab -->
              </div> <!-- /.c-item -->
            {{/ home-carousel }}
          </div> <!-- /.carousel--tabbed -->
        </div> <!-- /.shift-left-inner -->
      </div> <!-- /.shift-left -->

      <div class="shift-right home-right-blocks cf">
        {{# tip }}
          {{> molecules-block-thumb-headline }}
        {{/ tip }}
        {{> atoms-ad-300x250 }}
      </div> <!-- /.shift-right -->
    </div> <!-- /.lc-inner--l -->

    <section class="section promo-strip">
      <div class="lc-inner--m">
        <div class="g g-3up">
          {{# promo-strip }}
            <div class="gi">
              {{> molecules-block-thumb-headline }}
            </div> <!-- /.gi -->
          {{/ promo-strip }}
        </div> <!-- /.g-3up -->
      </div> <!-- /.lc-inner--m -->
    </section> <!-- /.promo-strip -->

    {{# has-btm-ad }}
      <div class="lc-inner--m">
        <div class="advertisement advertisement-responsive-leaderboard ad-blocks ad-leaderboard {{# has-994x250 }} has-994x250 {{/ has-994x250 }}">
          <div class="ad-block ad-300x50 js-ad-refresh"><a href=""><img src="http://cdn.creative.medialytics.com/raw_assets/servo_prod/campaigns/CA-KS2PC8D/creatives/CR-CDTH2C5/versions/1/MBAN%20UnstopablesTargetiMedia%20FreshScentspiration%20xxxx%20320x50%201415%203%20Static.jpg.jpg"></a></div>
          {{# has-994x250 }}
            <div class="ad-block ad-994x250"><a href=""><img src="http://s0.2mdn.net/4764406/994x250_OWO_OpeningMay29-UPDATED.jpg" width="994" height="250" border="0"></a></div>
          {{/ has-994x250 }}
          {{^ has-994x250 }}
            <div class="ad-block ad-728x90 js-ad-refresh"><a href=""><img src="http://pagead2.googlesyndication.com/pagead/imgad?id=CICAgKDjt-P9LBABGAEyCHdr0udyiur2"></a></div>
          {{/ has-994x250 }}
        </div> <!-- /.ad-leaderboard -->
        <hr class="space-and-half-btm space-and-half-top">
      </div> <!-- /.lc-inner--m -->
    {{/ has-btm-ad }}

    <div class="lc cf">
      <div class="primary no-space-btm no-pad-btm">
        <div class="primary-inner no-space-btm">
          <section class="section content-modules">
            {{# content-modules-1 }}
              {{> molecules-block-thumb-headline }}
            {{/ content-modules-1 }}
          </section> <!-- /.content-modules -->

          {{# has-follow }}
            <hr class="space-btm no-space-top">
            {{> organisms-follow-footer }}
            <hr class="space-half-top">
          {{/ has-follow }}

          {{# has-video-strip }}
            <hr class="space-and-half-btm no-space-top">
            <section class="section video-strip">
              <header class="s-header rel">
                <h3 class="s-title heading">Featured Videos</h3>
                <div class="sponsored-by center">
                  <div class="dib">
                    <a href=""><img src="http://placehold.it/170x30"></a>
                  </div> <!-- /.dib -->
                </div> <!-- /.sponsored-by -->
              </header> <!-- /.s-header -->
              <div class="g g-3up">
                {{# video-strip }}
                  <div class="gi">
                    {{> molecules-block-thumb-headline }}
                  </div> <!-- /.gi -->
                {{/ video-strip }}
              </div> <!-- /.g-3up -->
            </section> <!-- /.video-strip -->
            <hr class="space-half-top">
          {{/ has-video-strip }}

          <section class="section content-modules">
            {{# content-modules-2 }}
              {{> molecules-block-thumb-headline }}
            {{/ content-modules-2 }}
          </section> <!-- /.content-modules -->
        </div> <!-- /.primary-inner -->
      </div> <!-- /.primary -->

      <div class="secondary no-pad-top">
        <aside class="sidebar sidebar--alt" role="complementary">
          {{# subscription-promo }}
            {{> molecules-block-thumb-headline }}
          {{/ subscription-promo }}
          {{> organisms-daily-finds }}
          <div class="ad-block ad-300x600 block">
            <a href=""><img src="http://s1.2mdn.net/viewad/3521978/SINS_WebBanner_300x600.jpg"></a>
          </div> <!-- /.ad-300x250 -->
        </aside> <!-- /.sidebar -->
      </div> <!-- /.secondary -->
      <hr class="no-space-top space-double-btm clear lc-inner--l">
    </div> <!-- /.lc -->

    <div class="lc lc-inner--l cf page--btm">
      <section class="section video-promo cf v-blocks--3">
        <div class="video-module">
          {{# video }}
            {{> molecules-block-media }}
          {{/ video }}
        </div> <!-- /.video-module -->
        <div class="video-blocks">
          {{# video-blocks }}
            {{> molecules-block-thumb-headline }}
          {{/ video-blocks }}
        </div> <!-- /.video-blocks -->
      </section> <!-- /.video-promo -->
    </div> <!-- /.lc-inner--l -->

  </div><!-- /role=main -->

  <div class="lc lc-inner space-double-btm pad-half-top">
    <hr class="hr--s no-space-top space-btm">
    <div class="ad-blocks ad-responsive ad-instream">
      <div class="ad-block ad-300x250 hide-larger hide-large"><a href=""><img src="http://dqckmuwjiytix.cloudfront.net/a/8933/img/300x250_Free%20Sample_static.jpg"></a></div>
      <div class="ad-block ad-728x90 hide-larger hide-med hide-small"><a href=""><img src="http://s1.2mdn.net/viewad/3340030/clx_clb_e40024_value-oxi_rev_728x90.jpg"></a></div>
      <div class="ad-block ad-970x90 hide-large hide-med hide-small"><a href=""><img src="http://d.pr/i/10NZe+"></a></div>
    </div> <!-- /.ad-responsive -->
    <hr class="hr--l space-half-top">
  </div> <!-- /.lc-inner -->

  {{> organisms-footer }}
</div> <!-- /.page -->
