&lt;article class=&quot;slide {{ slide-class }}&quot; data-url=&quot;{{ data-url }}&quot;&gt;
  &lt;div class=&quot;slide-inner cf&quot;&gt;

    {{^ is-ad-slide }}
      &lt;div class=&quot;slide-img&quot;&gt;
        &lt;div class=&quot;hover-slide-navigation&quot;&gt;
          &lt;span class=&quot;icon no-bg icon-arrow-left js-slide-hover-prev&quot;&gt;{{&gt; atoms-arrow-left }}&lt;/span&gt;
          &lt;span class=&quot;icon no-bg icon-arrow-right js-slide-hover-next&quot;&gt;{{&gt; atoms-arrow-right }}&lt;/span&gt;
        &lt;/div&gt; &lt;!-- /.hover-slide-navigation --&gt;
        {{&gt; molecules-image-with-social }}
      &lt;/div&gt; &lt;!-- /.slide-img --&gt;
    {{/ is-ad-slide }}

    {{# is-welcome-slide }}
      &lt;div class=&quot;slide-intro slide-text&quot;&gt;
        &lt;p class=&quot;dek&quot;&gt;{{ slide-intro }}&lt;/p&gt;
        {{&gt; molecules-byline }}
        &lt;div class=&quot;btn btn--gallery-start js-btn--gallery-start&quot;&gt;Start &lt;span class=&quot;icon icon-arrow-right no-bg&quot;&gt;{{&gt; atoms-arrow-right }}&lt;/span&gt;&lt;/div&gt;
      &lt;/div&gt; &lt;!-- /.slide-intro --&gt;
    {{/ is-welcome-slide }}

    {{# is-normal-slide }}
      &lt;div class=&quot;slide-text&quot;&gt;
        &lt;div class=&quot;slide-controls&quot;&gt;
          {{&gt; molecules-slide-counter }}
          &lt;a href=&quot;{{ url }}&quot; class=&quot;see-all--grid js-toggle&quot;&gt;&lt;span class=&quot;icon icon-grid icon--small&quot;&gt;{{&gt; atoms-grid }}&lt;/span&gt; &lt;span class=&quot;light&quot;&gt;See All&lt;/span&gt;&lt;/a&gt;
        &lt;/div&gt; &lt;!-- /.slide-controls --&gt;
        &lt;div class=&quot;text&quot;&gt;
          &lt;h2 class=&quot;slide-title&quot;&gt;{{ slide-title }}&lt;/h2&gt;
          &lt;p&gt;{{{ slide-dek }}}&lt;/p&gt;
        &lt;/div&gt;
      &lt;/div&gt; &lt;!-- /.slide-text --&gt;
    {{/ is-normal-slide }}

    {{# is-ad-slide }}
      &lt;div class=&quot;slide-controls&quot;&gt;
        &lt;div class=&quot;slide-navigation&quot;&gt;
          &lt;span class=&quot;icon no-bg rounded icon-arrow-left icon--alt icon--medium js-slide-hover-prev&quot;&gt;{{&gt; atoms-arrow-left }}&lt;/span&gt;
          &lt;span class=&quot;icon no-bg rounded icon-arrow-right icon--alt icon--medium js-slide-hover-next&quot;&gt;{{&gt; atoms-arrow-right }}&lt;/span&gt;
          &lt;div class=&quot;slide-count&quot;&gt;Skip Ad&lt;/div&gt;
        &lt;/div&gt; &lt;!-- /.slide-navigation --&gt;
      &lt;/div&gt; &lt;!-- /.slide-controls --&gt;
      &lt;div class=&quot;ad-block-wrap&quot;&gt;
        {{&gt; atoms-ad-300x250 }}
      &lt;/div&gt; &lt;!-- /.ad-block-wrap --&gt;
    {{/ is-ad-slide }}

  &lt;/div&gt; &lt;!-- /.slide-inner --&gt;
&lt;/article&gt; &lt;!-- /.slide --&gt;
