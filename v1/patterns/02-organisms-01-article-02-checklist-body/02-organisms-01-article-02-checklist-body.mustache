{{# checklist }}
  &lt;section class=&quot;section checklist-items&quot;&gt;
    &lt;header class=&quot;s-header&quot;&gt;
      &lt;h2 class=&quot;s-title alt-heading--pink align-left&quot;&gt;{{ cl-title }}&lt;/h2&gt;
    &lt;/header&gt; &lt;!-- /.s-header --&gt;
    &lt;ol class=&quot;cl-task-list&quot;&gt;
      {{# cl-items }}
        {{# is-cl-item }}
          &lt;li class=&quot;cl-item js-toggle {{ cl-item-class }}&quot;&gt;
            &lt;div class=&quot;cl-task&quot;&gt;
              &lt;span class=&quot;cb cb--large&quot;&gt;{{&gt; atoms-check }}&lt;/span&gt;
              &lt;strong class=&quot;has-strike text--l&quot;&gt;&lt;span&gt;{{ cl-task }}&lt;/span&gt;&lt;/strong&gt;
            &lt;/div&gt; &lt;!-- /.cl-task --&gt;
            {{# cl-desc }}
              &lt;div class=&quot;cl-desc text&quot;&gt;
                &lt;p class=&quot;text--r&quot;&gt;{{ cl-desc }}&lt;/p&gt;
              &lt;/div&gt; &lt;!-- /.cl-desc --&gt;
            {{/ cl-desc }}
          &lt;/li&gt; &lt;!-- .cl-item --&gt;
        {{/ is-cl-item }}
        {{^ is-cl-item }}
          &lt;li class=&quot;space-top&quot;&gt;
            &lt;div class=&quot;ad-blocks ad-responsive ad-instream&quot;&gt;
              &lt;div class=&quot;ad-block ad-300x250 hide-larger&quot;&gt;&lt;a href=&quot;&quot;&gt;&lt;img src=&quot;http://dqckmuwjiytix.cloudfront.net/a/8933/img/300x250_Free%20Sample_static.jpg&quot;&gt;&lt;/a&gt;&lt;/div&gt;
              &lt;div class=&quot;ad-block ad-728x90 hide-large hide-med hide-small&quot;&gt;&lt;a href=&quot;&quot;&gt;&lt;img src=&quot;http://s1.2mdn.net/viewad/3340030/clx_clb_e40024_value-oxi_rev_728x90.jpg&quot;&gt;&lt;/a&gt;&lt;/div&gt;
            &lt;/div&gt; &lt;!-- /.ad-responsive --&gt;
          &lt;/li&gt; &lt;!-- .cl-item --&gt;
        {{/ is-cl-item }}
      {{/ cl-items }}
    &lt;/ol&gt; &lt;!-- /.cl-task-list --&gt;
  &lt;/section&gt; &lt;!-- /.checklist-items --&gt;
{{/ checklist }}
