&lt;div class=&quot;block block-thumb {{ block-class }}&quot;&gt;
  {{# kicker-top }}
    {{&gt; molecules-kicker }}
  {{/ kicker-top }}

  {{# kicker-btm }}
  &lt;div class=&quot;b-inner&quot;&gt;
  {{/ kicker-btm }}
  {{^ kicker-btm }}
  &lt;a href=&quot;{{ url }}&quot; class=&quot;b-inner&quot;&gt;
  {{/ kicker-btm }}

    &lt;div class=&quot;b-thumb&quot;&gt;

      {{# kicker-btm }}
      &lt;a href=&quot;{{ url }}&quot;&gt;
      {{/ kicker-btm }}

      &lt;div class=&quot;img-wrap&quot;&gt;
        {{# picturefill }}
          {{&gt; atoms-picturefill }}
        {{/ picturefill }}
        {{^ picturefill }}
          {{&gt; atoms-image }}
        {{/ picturefill }}

        {{# is-video }}
          &lt;span class=&quot;icon icon-play {{ video-icon-class }} rounded&quot;&gt;{{&gt; atoms-play }}&lt;/span&gt;
        {{/ is-video}}

      &lt;/div&gt; &lt;!-- /.img-wrap --&gt;

      {{# kicker-btm }}
      &lt;/a&gt;
      {{/ kicker-btm }}

    &lt;/div&gt; &lt;!-- /.b-thumb --&gt;
    &lt;div class=&quot;b-text&quot;&gt;
      {{# date }}
        {{&gt; atoms-time }}
      {{/ date }}
      {{# number }}
        &lt;span class=&quot;rounded number&quot;&gt;{{ number }}&lt;/span&gt;
      {{/ number }}

      {{# kicker-btm }}
        {{&gt; molecules-kicker }}
        {{# is_sponsored }}
          {{&gt; molecules-sponsor }}
        {{/ is_sponsored }}
      {{/ kicker-btm }}

      {{# kicker-btm }}
      &lt;a href=&quot;{{ url }}&quot;&gt;
      {{/ kicker-btm }}

      &lt;h3 class=&quot;b-title {{ title-class }}&quot;&gt;&lt;span&gt;{{{ title }}}&lt;/span&gt;&lt;/h3&gt;

      {{# kicker-btm }}
      &lt;/a&gt;
      {{/ kicker-btm }}

      {{# byline }}
        {{&gt; molecules-byline }}
      {{/ byline }}
      {{# dek }}
      &lt;div class=&quot;dek {{ dek-class }}&quot;&gt;
        &lt;p&gt;{{{ dek }}}&lt;/p&gt;
      &lt;/div&gt; &lt;!-- /.dek --&gt;
      {{/ dek }}
      {{# cta }}
        &lt;span class=&quot;cta&quot;&gt;{{ cta }}&lt;/span&gt;
      {{/ cta }}

      {{# runtime }}
        &lt;span class=&quot;light runtime {{ runtime-class }}&quot;&gt;({{ runtime }})&lt;/span&gt;
      {{/ runtime }}
    &lt;/div&gt; &lt;!-- /.b-text --&gt;

  {{# kicker-btm }}
  &lt;/div&gt; &lt;!-- /.b-inner --&gt;
  {{/ kicker-btm }}
  {{^ kicker-btm }}
  &lt;/a&gt; &lt;!-- /.b-inner --&gt;
  {{/ kicker-btm }}

&lt;/div&gt; &lt;!-- /.block-thumb --&gt;
